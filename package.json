{
	"name": "icarus",
	"version": "1.0.0",
	"description": "Open source FiveM anticheat",
	"main": "index.js",
	"scripts": {
		"server": "esbuild --bundle --minify --keep-names --packages=external --platform=node --target=node16.9.1 src/App.ts --outfile=dist/server.js",
		"client": "esbuild --bundle ui/nui/NuiCallbackService.ts --outfile=dist/nui.js && esbuild --bundle ui/Index.ts --outfile=dist/ui.js",
		"setup": "copy ui\\views\\index.html .\\dist\\ && copy ui\\views\\style.css .\\dist\\",
		"build": "npm run server && npm run client && npm run setup"
	},
	"author": "EinS4ckZwiebeln",
	"license": "MIT",
	"devDependencies": {
		"@types/bootstrap": "^5.0.2",
		"@types/chart.js": "^2.9.41",
		"@types/jquery": "^3.5.29",
		"@types/node": "^20.10.6",
		"esbuild": "^0.19.8",
		"typescript": "5.3.3"
	},
	"dependencies": {
		"@citizenfx/client": "^2.0.7257-1",
		"@citizenfx/server": "^2.0.6736-1",
		"@popperjs/core": "^2.11.8",
		"@types/bad-words": "^3.0.3",
		"@types/node-fetch": "^2.6.9",
		"bad-words": "^3.0.4",
		"bootstrap": "^5.3.2",
		"chart.js": "^4.4.1",
		"form-data": "^4.0.0",
		"node-fetch": "^1.7.3",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^4.7.1"
	}
}
